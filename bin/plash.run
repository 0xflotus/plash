#!/usr/bin/python3
import argparse
import atexit
import os
import subprocess
import sys
import tempfile
import uuid

from plash import utils
from plash.core import Container
from plash.utils import get_subcommand_path

container_id = sys.argv[1]
cmd = sys.argv[2:]
if not cmd:
    print(container_id)
else:
    c = Container(container_id)
    c.die_if_not_builded()
    c.run(cmd)
